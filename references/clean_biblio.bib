  eprint = {1806.01655},
  keywords = {deep\_learning, gaussian\_processes},
  month = jun,
  note = {arXiv:1806.01655},
  posted-at = {2018-08-27 10:54:32},
  priority = {2},
  title = {{Deep Gaussian Processes with Convolutional Kernels}},
  year = {2018}
}

