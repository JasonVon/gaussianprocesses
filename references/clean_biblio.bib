@inproceedings{Guo17,
  abstract = {{Confidence calibration ‚Äì the problem of predicting probability estimates representative of the true correctness likelihood ‚Äì is important for classification models in many applications. We discover that modern neural networks, unlike those from a decade ago, are poorly calibrated. Through extensive experiments, we observe that depth, width, weight decay, and Batch Normalization are important factors influencing calibration. We evaluate the performance of various post-processing calibration methods on state-of-the-art architectures with image and document classification datasets. Our analysis and experiments not only offer insights into neural network learning, but also provide a simple and straightforward recipe for practical settings: on most datasets, temperature scaling ‚Äì a single-parameter variant of Platt Scaling ‚Äì is surprisingly effective at calibrating predictions.}},
  address = {International Convention Centre, Sydney, Australia},
  author = {Guo, Chuan and Pleiss, Geoff and Sun, Yu and Weinberger, Kilian Q.},
